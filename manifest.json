{
    "manifest_version": 2,
    "name": "Alt Hover",
    "version": "0.1",
    "description": "Display Twitter image descriptions on hover",
    "content_scripts": [
        {
            "matches": ["https://*.twitter.com/*", "http://*.twitter.com/*"],
            "css": ["bootstrap.min.css"],
            "js": ["jquery-3.3.1.min.js", "popper.min.js", "tooltip.min.js", "bootstrap.min.js", "content.js"]
        }
    ]
}
